{
    "task": "GLUE",
    "attention_probs_dropout_prob": 0.1,
    "hidden_act": "gelu",
    "hidden_dropout_prob": 0.1,
    "hidden_size": 1024,
    "initializer_range": 0.02,
    "intermediate_size": 4096,
    "max_position_embeddings": 512,
    "max_predictions_per_seq": 128,
    "vocab_size": 30400,
    "num_attention_heads": 16,
    "num_hidden_layers": 1,
    "type_vocab_size": 2,
    "seq_length": 384,
    "use_attention_projection_bias": true,
    "use_cls_layer": false,
    "pipeline_stages": [
     ["emb", "pos"],
     ["hid"],
     ["glu"]
    ],
    "device_mapping":[0,1,0],
    "micro_batch_size": 3,
    "device_iterations": 1,
    "epochs": 3,
    "epsilon": 0.000001,
    "lr_schedule": "polynomial_decay",
    "base_learning_rate": 2e-5,
    "warmup": 0.1,
    "loss_scaling": 1,
    "optimizer": "adamw",
    "beta1": 0.9,
    "beta2": 0.999,
    "gradient_accumulation_count": 6,
    "pipeline_schedule": "Grouped",
    "replicas": 1,
    "partials_type":"half",
    "reduction_type": "mean",
    "precision": "16",
    "buffer_size": 100000000,
    "seed": 7801,
    "steps_per_ckpts": 99999,
    "steps_per_logs": 1,
    "weight_decay_rate": 0.01,
    "available_memory_proportion": 0.15,
    "parallel_io_threads": 16,
    "matmul_serialize_factor":5,
    "duplicate_factor":5,
    "no_outlining": false,
    "stochastic_rounding": true,
    "enable_recomputation": true,
    "fp_exceptions": false,

    "steps_per_tensorboard": -1,

    "static_mask": false,
    "do_training": false,
    "do_evaluation": false,
    "do_predict": false,
    "do_lower_case": true,
    "verbose_logging": false,
    "variable_offloading": true,

    "output_dir": "tmp/glue/",
    "save_path": "checkpoint/glue",
    "vocab_file": "vocab.txt",
    "tfrecord_dir": "tmp/tf_record_cache/"



    
}

