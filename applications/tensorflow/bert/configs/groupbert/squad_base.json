{
    "task": "SQuAD",
    "attention_probs_dropout_prob": 0.1,
    "hidden_act": "gelu",
    "hidden_dropout_prob": 0.1,
    "hidden_size": 768,
    "initializer_range": 0.02,
    "intermediate_size": 3072,
    "max_position_embeddings": 512,
    "vocab_size": 30528,
    "num_attention_heads": 12,
    "num_hidden_layers": 12,
    "type_vocab_size": 2,
    "seq_length": 384,
    "use_attention_projection_bias": true,
    "use_cls_layer": false,
    "pipeline_stages": [
     ["emb", "pos"],
     ["hid","hid", "hid"],
     ["hid","hid", "hid"],
     ["hid","hid", "hid"],
     ["hid","hid", "hid"],
     ["loc"]],
    "device_mapping":[0,1,2,3,4,0],
    "micro_batch_size": 1,
    "batches_per_step": 1,
    "epochs": 3,
    "epsilon": 0.000001,
    "lr_schedule": "polynomial_decay",
    "base_learning_rate": 1.5e-4,
    "warmup": 0.1,
    "loss_scaling": 512,
    "optimizer": "mpadamw",
    "beta1": 0.9,
    "beta2": 0.999,
    "gradient_accumulation_count": 36,
    "pipeline_schedule": "Grouped",
    "replicas": 1,
    "partials_type":"half",
    "reduction_type": "mean",

    "precision": "16",
    "seed": 1234,
    "steps_per_ckpts": 100,
    "steps_per_logs": 1,
    "weight_decay_rate": 0.01,
    "available_memory_proportion": ["0.16", "0.20", "0.20", "0.20", "0.20", "0.16"],
    "parallel_io_threads": 16,
    "matmul_serialize_factor":6,
    "duplicate_factor":5,
    "no_outlining": false,
    "stochastic_rounding": true,
    "enable_recomputation": true,
    "fp_exceptions": false,

    "init_checkpoint": "",

    "steps_per_tensorboard": -1,
    "static_mask": false, 

    "do_training": false,
    "do_predict": false,
    "do_evaluation": true,
    "do_lower_case": true,
    "verbose_logging": false,
    "version_2_with_negative": false,
    "variable_offloading": true,

    "output_dir": null,
    "save_path": "./checkpoint/squad",
    "vocab_file": "./data/uncased_L-2_H-128_A-2/vocab.txt",
    "train_file": "./data/squad/train-v1.1.json",
    "predict_file": "data/squad/dev-v1.1.json",
    "tfrecord_dir": "./tmp/tf_record_cache/",

    "groupbert_conv_group_size": 16,
    "use_debiasing": true,
    "groupbert": true,
    "groupbert_conv_kernel_size": 7,
    "groupbert_ffn_output_groups": 4
}

