{
    "task": "SQuAD",
    "attention_probs_dropout_prob": 0.1,
    "hidden_act": "gelu",
    "hidden_dropout_prob": 0.1,
    "hidden_size": 768,
    "initializer_range": 0.02,
    "intermediate_size": 3072,
    "max_position_embeddings": 512,
    "vocab_size": 30400,
    "num_attention_heads": 12,
    "num_hidden_layers": 12,
    "type_vocab_size": 2,
    "seq_length": 384,
    "use_attention_projection_bias": false,
    "use_cls_layer": false,
    "use_qkv_bias": false,
    "use_qkv_split": true,
    "pipeline_stages": [
        ["emb", "pos"],
        ["hid","hid", "hid", "hid"],
        ["hid","hid", "hid", "hid"],
        ["hid","hid", "hid", "hid"],
        ["loc"]
    ],
    "device_mapping":[0, 0, 0, 0, 0],
    "batch_size": 4,
    "batches_per_step": 2,
    "num_train_steps": 2100,
    "epsilon": 0.0001,
    "lr_schedule": "polynomial_decay",
    "base_learning_rate": 3e-5,
    "warmup": 276,
    "loss_scaling": 1,
    "optimizer": "adamw",
    "beta1": 0.9,
    "beta2": 0.999,
    "gradient_accumulation_count": 10,
    "pipeline_schedule": "Grouped",
    "replicas": 1,
    "partials_type":"half",
    "reduction_type": "mean",

    "precision": "16",
    "seed": 1234,
    "steps_per_ckpts": 100,
    "steps_per_logs": 1,
    "weight_decay_rate": 0.01,
    "available_memory_proportion": 0.4,
    "doc_stride":128,
    "max_query_length":64,
    "parallel_io_threads": 1,
    "matmul_serialize_factor": 4,
    "no_outlining": true,
    "stochastic_rounding": false,
    "enable_recomputation": true,
    "fp_exceptions": false,

    "init_checkpoint": "converted_ckpt/bert_model.ckpt",

    "static_mask": false,

    "do_training": false,
    "do_predict": true,
    "do_lower_case": true,
    "verbose_logging": false,
    "version_2_with_negative": false,
    "variable_offloading": false,

    "output_dir": "./tmp/squad/",
    "save_path": "./checkpoint/squad",
    "vocab_file": "data/uncased_L-12_H-768_A-12/vocab.txt",
    "train_file": "data/squad/train-v1.1.json",
    "predict_file": "data/squad/dev-v1.1.json",
    "tfrecord_dir": "./tmp/tf_record_cache/"
}

