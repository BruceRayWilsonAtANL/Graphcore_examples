{
    "task": "SQuAD",
    "attention_probs_dropout_prob": 0.1,
    "hidden_act": "gelu",
    "hidden_dropout_prob": 0.1,
    "hidden_size": 768,
    "initializer_range": 0.02,
    "intermediate_size": 3072,
    "max_position_embeddings": 512,
    "vocab_size": 30400,
    "num_attention_heads": 12,
    "num_hidden_layers": 1,
    "type_vocab_size": 2,
    "seq_length": 384,
    "max_predictions_per_seq": 56,
    "use_attention_projection_bias": false,
    "use_cls_layer": false,
    "pipeline_stages": [
     ["emb", "pos", "hid"],
     ["loc"]],
    "device_mapping":[0,0],
    "micro_batch_size": 1,
    "batches_per_step": 1,
    "num_train_steps": 2100,
    "epsilon": 0.0001,
    "lr_schedule": "polynomial_decay",
    "base_learning_rate": 3e-5,
    "warmup": 276,
    "loss_scaling": 1,
    "optimizer": "adamw",
    "beta1": 0.9,
    "beta2": 0.999,
    "gradient_accumulation_count": 260,
    "pipeline_schedule": "Grouped",
    "replicas": 4,
    "partials_type":"half",
    "reduction_type": "mean",

    "precision": "16",
    "seed": 1234,
    "steps_per_ckpts": 100,
    "steps_per_logs": 1,
    "weight_decay_rate": 0.01,
    "available_memory_proportion": 0.28,
    "parallel_io_threads": 16,
    "matmul_serialize_factor":5,
    "duplicate_factor":5,
    "no_outlining": false,
    "stochastic_rounding": true,
    "enable_recomputation": true,
    "fp_exceptions": true,

    "init_checkpoint": "",

    "static_mask": false, 

    "do_training": true,
    "do_predict": false,
    "do_lower_case": true,
    "verbose_logging": false,
    "version_2_with_negative": false,
    "variable_offloading": false,

    "output_dir": "./tmp/squad/",
    "save_path": "./checkpoint/squad",
    "vocab_file": "data/ckpts/uncased_L-12_H-768_A-12/vocab.txt",
    "train_file": "data/squad/train-v1.1.json",
    "predict_file": "data/squad/dev-v1.1.json",
    "tfrecord_dir": "./tmp/tf_record_cache/"
}

